{
  "name": "vanillajs-form-validator",
  "version": "1.0.7",
  "description": "Form validation in pure JavaScript.",
  "main": "./src/index.js",
  "scripts": {
    "start": "webpack-dev-server --config ./webpack/dev.config.js",
    "start:open": "npm start --open",
    "test": "jest --coverage",
    "test:watch": "npm test -- --watchAll",
    "lint": "standard --verbose | snazzy",
    "lint:fix": "standard --fix",
    "lint:watch": "gulp",
    "build": "webpack --config ./webpack/prod.config.js --mode production",
    "precommit": "npm run lint:fix && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ederssouza/vanillajs-form-validator.git"
  },
  "keywords": [
    "form validation",
    "js",
    "library"
  ],
  "author": "Eder Sampaio <edersampaio@outlook.com.br>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ederssouza/vanillajs-form-validator/issues"
  },
  "homepage": "https://github.com/ederssouza/vanillajs-form-validator#readme",
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-core": "^6.26.3",
    "babel-jest": "^23.6.0",
    "babel-loader": "^7.1.5",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "chai": "^4.1.2",
    "clean-webpack-plugin": "^0.1.19",
    "coveralls": "^3.0.2",
    "gulp": "^3.9.1",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^0.14.3",
    "jest-cli": "^23.6.0",
    "snazzy": "^8.0.0",
    "standard": "^12.0.1",
    "style-loader": "^0.23.0",
    "uglifyjs-webpack-plugin": "^2.0.1",
    "webpack": "^4.19.0",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.8"
  },
  "babel": {
    "presets": [
      "env"
    ]
  },
  "standard": {
    "globals": [
      "describe",
      "it",
      "test",
      "beforeEach"
    ],
    "ignore": [
      "dist/"
    ]
  }
}
